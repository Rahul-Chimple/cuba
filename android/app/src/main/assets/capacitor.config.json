{
	"appId": "org.chimple.bahama",
	"appName": "Chimple",
	"webDir": "build",
	"bundledWebRuntime": false,
	"server": {
		"hostname": "chimple"
	},
	"plugins": {
		"FirebaseAuthentication": {
			"skipNativeAuth": false,
			"providers": [
				"google.com",
				"phone"
			]
		},
		"FirebaseMessaging": {
			"presentationOptions": [
				"badge",
				"sound",
				"alert"
			]
		},
		"Keyboard": {},
		"LocalNotifications": {
			"smallIcon": "chimple_monkey_icon"
		}
	}
}
